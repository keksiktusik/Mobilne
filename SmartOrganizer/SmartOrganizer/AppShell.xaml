<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SmartOrganizer"
    x:Class="SmartOrganizer.AppShell"
    FlyoutBehavior="Disabled"
    Title="SmartOrganizer">

    <FlyoutItem Title="Kalendarz" Route="CalendarPage">
        <ShellContent ContentTemplate="{DataTemplate local:CalendarPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Nowa strona" Route="SettingsPage">
        <ShellContent ContentTemplate="{DataTemplate local:SettingsPage}" />
    </FlyoutItem>

</Shell>